<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PressWaveButton">
        <attr name="maxWaveRadius" format="dimension" />
        <attr name="waveColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RoundProgressLoadingView">
        <attr name="innerColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="progressWidth" format="dimension" />
        <attr name="maxProgress" format="integer" />
        <attr name="progress" format="integer"/>
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
    </declare-styleable>

    <declare-styleable name="BubbleView">
        <attr name="bubbleNumber" format="integer" />
        <attr name="minRadius" format="float" />
        <attr name="maxRadius" format="float" />
        <attr name="minSpeed" format="float" />
        <attr name="maxSpeed" format="float" />
        <attr name="bubbleSrc" format="color|reference" />
    </declare-styleable>

</resources>
